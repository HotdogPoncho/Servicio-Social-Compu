<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#202732"
    tools:context=".ActualizarDatos">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="105dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <EditText
                android:id="@+id/txtNumeroDeTrabajador"
                android:layout_width="270dp"
                android:layout_height="41dp"
                android:layout_marginLeft="68dp"
                android:layout_marginTop="75dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                android:textColor="#FFFFFF" />

            <EditText
                android:id="@+id/txtApellidos"
                android:layout_width="270dp"
                android:layout_height="41dp"
                android:layout_marginLeft="68dp"
                android:layout_marginTop="27dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                android:textColor="#FFFFFF" />

            <EditText
                android:id="@+id/txtNombre"
                android:layout_width="270dp"
                android:layout_height="41dp"
                android:layout_marginLeft="68dp"
                android:layout_marginTop="27dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                android:textColor="#FFFFFF" />

            <EditText
                android:id="@+id/txtfechaYHora"
                android:layout_width="270dp"
                android:layout_height="41dp"
                android:layout_marginLeft="68dp"
                android:layout_marginTop="27dp"
                android:ems="10"
                android:inputType="textPersonName"
                android:text=""
                android:textColor="#FFFFFF" />

            <EditText
                android:id="@+id/txtImpresiones"
                android:layout_width="270dp"
                android:layout_height="41dp"
                android:layout_marginLeft="68dp"
                android:layout_marginTop="27dp"
                android:ems="10"
                android:hint="Impresiones"
                android:inputType="number"
                android:text=""
                android:textColor="#FFFFFF"
                android:textColorHint="#FFFFFF" />

            <EditText
                android:id="@+id/txtObservaciones"
                android:layout_width="270dp"
                android:layout_height="41dp"
                android:layout_marginLeft="68dp"
                android:layout_marginTop="27dp"
                android:ems="10"
                android:hint="Observaciones"
                android:inputType="textPersonName"
                android:text=""
                android:textColor="#FFFFFF"
                android:textColorHint="#FFFFFF" />

            <Button
                android:id="@+id/cmdActualizar"
                android:layout_width="219dp"
                android:layout_height="48dp"
                android:layout_marginLeft="95dp"
                android:layout_marginTop="36dp"
                android:background="@drawable/button_rounded"
                android:text="Actualizar"
                android:textColor="#FFFFFF" />

            <Button
                android:id="@+id/cmdModificar"
                android:layout_width="219dp"
                android:layout_height="48dp"
                android:layout_marginLeft="95dp"
                android:layout_marginTop="25dp"
                android:background="@drawable/button_rounded_border"
                android:onClick="cerrar"
                android:text="Salir"
                android:textColor="#FFFFFF" />
        </LinearLayout>
    </ScrollView>

    <TextView
        android:id="@+id/lblnum"
        android:layout_width="95dp"
        android:layout_height="30dp"
        android:enabled="false"
        android:text=""
        android:textColor="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblApellidos"
        android:layout_width="102dp"
        android:layout_height="29dp"
        android:enabled="false"
        android:text=""
        android:textColor="#FFFFFF"

        app:layout_constraintStart_toEndOf="@+id/lblnum"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblNombre"
        android:layout_width="95dp"
        android:layout_height="30dp"
        android:enabled="false"
        android:text=""
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lblApellidos"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lblFechaYHora"
        android:layout_width="95dp"
        android:layout_height="30dp"
        android:enabled="false"
        android:text=""
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lblnum" />

    <TextView
        android:id="@+id/lblImpresiones"
        android:layout_width="95dp"
        android:layout_height="30dp"
        android:enabled="false"
        android:text=""
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintStart_toEndOf="@+id/lblFechaYHora" />

    <TextView
        android:id="@+id/lblObservaciones"
        android:layout_width="95dp"
        android:layout_height="30dp"
        android:enabled="false"
        android:text=""
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lblImpresiones" />
</androidx.constraintlayout.widget.ConstraintLayout>