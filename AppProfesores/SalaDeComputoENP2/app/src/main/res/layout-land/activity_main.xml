<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#202732"
    tools:context=".MainActivity">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="-16dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="301dp"
                android:layout_marginTop="26dp"
                android:layout_marginEnd="302dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/formicon" />

            <EditText
                android:id="@+id/txtNumTrab"
                android:layout_width="318dp"
                android:layout_height="42dp"
                android:layout_marginStart="206dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="207dp"
                android:background="@drawable/custom_input"
                android:drawableLeft="@drawable/custom_icon_baseline_bagde"
                android:drawablePadding="12dp"
                android:ems="10"
                android:hint="@string/txtNumTrab"
                android:importantForAutofill="no"
                android:inputType="number"
                android:paddingStart="12dp"
                android:paddingEnd="12dp"
                android:singleLine="true"
                android:textColor="#FFFFFF"
                android:textColorHint="#FFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView2"
                tools:ignore="TouchTargetSizeCheck" />

            <Button
                android:id="@+id/cmdIniciarSesion"
                android:layout_width="219dp"
                android:layout_height="48dp"
                android:layout_marginStart="256dp"
                android:layout_marginTop="36dp"
                android:layout_marginEnd="256dp"
                android:background="@drawable/button_rounded"
                android:onClick="IniciarSesion"
                android:text="@string/cmdIniciarSesion"
                android:textColor="#FFFFFF"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/txtNumTrab" />

            <Button
                android:id="@+id/cmdRegistrarse"
                android:layout_width="219dp"
                android:layout_height="48dp"
                android:layout_marginStart="256dp"
                android:layout_marginTop="24dp"
                android:layout_marginEnd="256dp"
                android:background="@drawable/button_rounded_border"
                android:onClick="formRegistro"
                android:text="@string/cmdRegistrarse"
                android:textColor="#FFFFFF"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/cmdIniciarSesion" />

        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <TextView
        android:id="@+id/lblApellidos"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:enabled="false"
        android:layout_marginLeft="92dp"
        android:text=""
        android:textColor="#FFFFFF"
        android:visibility="invisible"
        tools:layout_editor_absoluteY="661dp" />

    <TextView
        android:id="@+id/lblNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="92dp"
        android:enabled="false"
        android:text=""
        android:textColor="#FFFFFF"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="68dp"
        tools:layout_editor_absoluteY="554dp" />

    <TextView
        android:id="@+id/lblFecha"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="92dp"
        android:enabled="false"
        android:text=""
        android:visibility="invisible"
        tools:layout_editor_absoluteY="630dp" />

    <TextView
        android:id="@+id/lblHora"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginLeft="92dp"
        android:enabled="false"
        android:text=""
        android:visibility="invisible"
        tools:layout_editor_absoluteY="626dp" />


</RelativeLayout>